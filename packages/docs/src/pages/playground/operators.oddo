export default `// Operators in Oddo

// Arithmetic
x = 1 + 2 * 3
y = 10 - 5 / 2
z = x ** 2

// Comparison
isEqual = x == 7
isGreater = x > 5
isLess = y < 10

// Logical
a = true && false
b = true || false
c = !true

// Ternary
result = x > 5 ? "big" : "small"

// Pipe operator
double = (n) => n * 2
addOne = (n) => n + 1
piped = 5 |> double |> addOne

// Assignment operators
@mutable counter = 0
counter +:= 1
counter *:= 2`

